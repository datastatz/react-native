    "expo": "~50.0.17",
    "expo-linking": "^5.0.1",
    "expo-status-bar": "~1.11.1",
    "formik": "^2.4.6",
    "graphql": "^16.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-web": "~0.19.6",
    "react-router-native": "^6.30.0",
    "styled-components": "^6.1.8",
    "use-debounce": "^9.0.3",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/eslint-parser": "^7.27.0",
    "@testing-library/jest-native": "^5.4.2",
