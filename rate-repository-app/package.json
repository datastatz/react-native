  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.13.7",
    "@expo/metro-runtime": "~3.1.1",
    "@react-native-picker/picker": "^2.4.12",
